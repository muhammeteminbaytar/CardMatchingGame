<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".level3Act">

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lvl3gametimer">

        <ImageView
            android:id="@+id/lvl3iw1"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw2"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw3"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="0"
            app:layout_row="1"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw4"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="1"
            app:layout_row="1"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw5"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="0"
            app:layout_row="2"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw6"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="1"
            app:layout_row="2"
            app:srcCompat="@drawable/qcard" />


        <ImageView
            android:id="@+id/lvl3iw7"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="0"
            app:layout_row="3"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw8"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="1"
            app:layout_row="3"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw9"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="0"
            app:layout_row="4"
            app:srcCompat="@drawable/qcard" />

        <ImageView
            android:id="@+id/lvl3iw10"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="20dp"
            android:onClick="lvl3iwclick"
            android:visibility="visible"
            app:layout_column="1"
            app:layout_row="4"
            app:srcCompat="@drawable/qcard" />



    </androidx.gridlayout.widget.GridLayout>

    <TextView
        android:id="@+id/lvl3gametimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="20"
        android:textColor="#303231"
        android:textSize="36sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>